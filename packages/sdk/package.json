{
  "name": "@ui-tars/sdk",
  "version": "1.2.0-beta.17",
  "description": "A powerful cross-platform(ANY device/platform) toolkit for building GUI automation agents for UI-TARS",
  "repository": {
    "type": "git",
    "url": "https://github.com/bytedance/UI-TARS-desktop"
  },
  "bugs": {
    "url": "https://github.com/bytedance/UI-TARS-desktop/issues"
  },
  "keywords": [
    "AI",
    "SDK",
    "Operator",
    "UI-TARS"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "browser": {
    "./": "./dist/index.mjs",
    "./core": "./dist/core.mjs",
    "./constants": "./dist/constants.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "import": "./dist/core.mjs",
      "require": "./dist/core.js"
    },
    "./constants": {
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "prepare": "tsx scripts/sync-docs.ts && npm run build",
    "build": "tsup",
    "test:browser": "vitest --environment happy-dom",
    "test": "vitest --environment happy-dom && vitest --environment node"
  },
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "openai": "4.85.1",
    "jimp": "1.6.0",
    "async-retry": "1.3.3",
    "@ui-tars/shared": "workspace:*",
    "@ui-tars/action-parser": "workspace:*"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "vitest-browser-react": "^0.1.1",
    "@types/async-retry": "1.4.9",
    "happy-dom": "^17.1.1"
  }
}
