{
  "name": "@multimodal/agent",
  "version": "0.1.0",
  "description": "An event-stream driven agent framework for building effective multimodal Agents.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./_config": {
      "import": "./dist/_config/index.mjs",
      "require": "./dist/_config/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "_config": [
        "dist/_config/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "prepare": "npm run build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "zod-to-json-schema": "3.24.3",
    "openai": "4.87.3",
    "zod": "3.24.2",
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@types/json-schema": "^7.0.15",
    "@agent-infra/mcp-client": "workspace:*",
    "@agent-infra/logger": "workspace:*",
    "@multimodal/llm-client": "workspace:*"
  },
  "devDependencies": {
    "@rslib/core": "0.5.4",
    "@types/node": "^20.10.4",
    "typescript": "^5.5.3",
    "vitest": "^3.0.8"
  }
}