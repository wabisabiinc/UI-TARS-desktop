    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "paths": {
      "@multimodal/*": [
        "./packages/multimodal/*"
      ],
      "@agent-tars/*": [
        "./packages/multimodal/*"
      ],
    },
  },
  "exclude": ["apps", "packages"]
}
